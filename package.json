{
  "name": "elasticmagic",
  "version": "0.0.1",
  "description": "JS orm for elasticsearch.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kindritskyiMax/elasticmagic-js.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "ES_HOST=es6 jest",
    "test:integ": "ES_HOST=es6 jest tests/testCluster.spec.ts",
    "test:ci": "ES_HOST=localhost jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^6.8.4",
    "tslint": "^5.20.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "author": "Kindritskiy Maksym <kindritskiy.m@gmail.com>",
  "license": "Apache License 2.0",
  "keywords": [
    "elasticsearch",
    "elastic",
    "dsl",
    "kibana",
    "mapping",
    "REST",
    "search",
    "client",
    "index"
  ],
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
